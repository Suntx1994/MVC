@model List<Contoso.Model.People>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<body>
    <table class="table table-dark">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.DateOfBirth</td>
            </tr>
        }
    </table>
</body>
